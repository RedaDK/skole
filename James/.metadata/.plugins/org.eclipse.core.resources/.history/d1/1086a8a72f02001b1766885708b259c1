package bogÿvelser;

public class Main {
		  public static void main(String[] args) {

					int[] values = { 1, 2, 4 };
					int total = 0;
					for (int element : values) {
							  System.out.println(element);
							  total = total + element;
					}

					System.out.println("Sum: " + total);
		  }

}
