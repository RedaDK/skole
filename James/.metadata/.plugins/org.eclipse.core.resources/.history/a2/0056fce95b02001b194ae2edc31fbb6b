package opg2;

public class ArrayTest {

		public static void main(String[] args) {
				int[] arr = { 2, 3, 4, 5, 6, };
				int sum = sum(arr);
				System.out.println("Totalt " + sum);

				int[] arr1 = { 2, 6, 7, 8, 9 };
				System.out.println("Sum " + arr1 + sum);
		}

		public static int sum(int[] t) {
				int total = 0;
				for (int i = 0; i < t.length; i++) {
						total = t[i] + total;
				}
				return total;
		}

//	} 
