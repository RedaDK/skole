package opg2;

public class ArrayTest {


		
		public static int sum(int[] t ) {
		
			t = new int [] {4,6,7,2,3};
			int sum = 0;
			for (int i : t) {
			    sum += i;
			}

			return sum;
	} 
		
		public static double sumDouble(double[] t ) {
			
			t = new double [] {2.3,6.6,7.2,2.9,3.1};
			int sumDouble = 0;
			for (double i : t) {
			    sumDouble += i;
			}

			return sumDouble;
	} 

}
