package sep05ex1student;

public class EmployeeApp
{
    public static void main(String[] args)
    {
        Employee e1 = new Employee("Hans Jensen", "Paulsen");
        e1.setAge(34);
        e1.printEmployee();

        e1.setName("Viggo Diggo");
        e1.setAge(45);
        e1.printEmployee();
        
        e1.setName("Jean Paul");
        e1.setAge(23);
        e1.printEmployee();
    }

}
