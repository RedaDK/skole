		public static int[] sumArrays(int[] a, int[] b) {
			
			int[] sum = new int [a.length];
			
			for(int i = 0; i < a.length; i++) {
				
					sum[i] = a[i] + b[i];
					//System.out.println(sum[i]);
				}
			return sum;
			}
		
	}



