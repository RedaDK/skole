package test;



public class Test {

public static int findMatchCount(final String [] [] ans,  final String [] correct){
    int matchCount = 0;

    for(int i = 0, j = 0;i < ans.length && j < correct.length;){
    	int res = ans[i] [j].compareTo(correct[j]);
        if(res == 0){
            matchCount++;
            i++;
            j++;
        }else if(res < 0){
            i++;
        }else{
            j++;
        }
    }
    return matchCount;
}


	public static void main(String[] args){ 
	    String[] ans = {"A","C","B","D","A","C","B","B"};
	    String[] correct = {"A","C","B","D","A","C","B","B"};
	    System.out.println(findMatchCount(ans,correct));

	}
}	