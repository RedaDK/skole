package opg2;

import java.util.Arrays;

public class Main {

	public static void main(String[] args) {
		
		String [][] ans = {	{"A","C","B","D","A","C","B","B"},
							{"C","B","C","A","C","D","A","B"},
							{"B","B","A","C","B","C","D","A"},
							{"C","A","C","B","B","D","A","C"},
							{"D","B","C","A","C","D","B","C"},
							{"A","C","A","D","A","A","D","A"},
							{"B","B","B","C","D","A","C","C"},
							{"D","B","C","C","A","B","A","A"},
							{"B","C","D","A","C","D","B","B"},
							{"C","A","B","C","D","C","B","A"},
				
						};

		String [] correct = {"A","C","B","D","A","C","B","B"};
			print(ans);
			//printOne(correct);
			
			correctAnswer(ans, correct);
	}
	
//*****************************************************************************************************/
	
	public static void print(String[][] ans) {
		
		for (int row = 0; row < ans.length; row++) {
			for (int col = 0; col < ans[row].length; col++) {
			}
			System.out.println();
		}
	}

		
	public static void correctAnswer(String[] [] ans, String [] correct) {
		String[] result = new String[8];
		for(int i = 0; i < ans.length; i++) {
			if(ans[0][i] == correct[i])
			 result [i] = ans[1] [i];
			 System.out.println(Arrays.deepToString(result));
		
		
		
	}
	
	}
}


